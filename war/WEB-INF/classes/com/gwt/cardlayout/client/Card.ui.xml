<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	
	<ui:with type="com.gwt.cardlayout.resources.ICardResources" field="resource"></ui:with>
	
	<g:HTMLPanel styleName="{resource.getStyle.panel}">
		<g:HTMLPanel ui:field="header" styleName="{resource.getStyle.header}">
			<g:HTMLPanel ui:field="headerText" styleName="{resource.getStyle.text}"/>
			<g:HTMLPanel ui:field="buttonBar" styleName="{resource.getStyle.buttonBar}">
				<g:FocusPanel ui:field="refresh" addStyleNames="{resource.getStyle.refresh} {resource.getStyle.button}"/>
				<g:FocusPanel ui:field="expand" addStyleNames="{resource.getStyle.expand} {resource.getStyle.button}"/>
			</g:HTMLPanel>				
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="content" styleName="{resource.getStyle.content}"/>			
	</g:HTMLPanel>
</ui:UiBinder> 